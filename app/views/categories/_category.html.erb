<div id="<%= dom_id(category) %>" class="category">
  <p>
    <%= image_tag(category.icon, class: "category_icon") %>
  </p>
  <div class="category-div">
  <p>
    <%= link_to(category.name, category_data_path(category.id), class: 'category_name') %>
  </p>
  <p class="time"> <%= category.created_at.strftime("%d %b %Y") %></p>
  </div>
  <p>
    $<%= category.data.sum(:amount) %>
  </p>
</div>