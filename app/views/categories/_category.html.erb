<div id="<%= dom_id category %>">
  <p>
    <strong>Name:</strong>
    <%= link_to "#{category.name}", category_data_path(category.id)%>
  </p>

  <p>
    <strong>Icon:</strong>
    <%= image_tag category.icon, class:"category_icon" %>
  </p>

  <p>
    <strong>User:</strong>
    <%= category.user_id %>
  </p>

</div>
